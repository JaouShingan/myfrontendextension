{
  ".": {
    "prefix": ".",
    "body": [
      ".${1:class-name} {",
      "\t${0}",
      "}"
    ],
    "description": "css base"
  },
  ".p": {
    "prefix": ".p",
    "body": [
      ".p-${1:class-name} {",
      "\t${0}",
      "}"
    ],
    "description": "css page"
  },
  ".g": {
    "prefix": ".g",
    "body": [
      ".g-${1:class-name} {",
      "\t${0}",
      "}"
    ],
    "description": "css layout"
  },
  ".m": {
    "prefix": ".m",
    "body": [
      ".m-${1:class-name} {",
      "\t${0}",
      "}"
    ],
    "description": "css module"
  },
  ".u": {
    "prefix": ".u",
    "body": [
      ".u-${1:class-name} {",
      "\t${0}",
      "}"
    ],
    "description": "css unit"
  },
  "flex center": {
    "prefix": "flexcenter",
    "body": [
      "display: flex;",
      "justify-content: center;",
      "align-items: center;\n"
    ],
    "description": "flex 居中布局"
  },
  "flex column": {
    "prefix": "flexcol",
    "body": [
      "display: flex;",
      "flex-direction: column;"
    ],
    "description": "flex 纵向布局"
  },
  "ellipsis": {
    "prefix": "ellipsis",
    "body": [
      "width: ${1:100px};",
      "white-space: nowrap;",
      "overflow: hidden;",
      "text-overflow: ellipsis;"
    ],
    "description": "单行文本使用省略符号"
  }
