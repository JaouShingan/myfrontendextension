{
	"name": "myfrontendextension",
	"displayName": "MyFrontendExtension",
	"description": "My front end extension",
	"publisher": "shinji",
	"license": "MIT",
	"repository": "https://github.com/JaouShingan/myfrontendextension",
	"version": "0.0.9",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:myfrontendextension.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "myfrontendextension.helloWorld",
				"title": "Hello World"
			}
		],
		"menus": {},
		"snippets": [
			{
				"language": "vue",
				"path": "./snippets/vue.json"
			},
			{
				"language": "javascript",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "javascriptreact",
				"path": "./snippets/javascript.json"
			},
			{
				"language": "vue-html",
				"path": "./snippets/vue-html.json"
			},
			{
				"language": "css",
				"path": "./snippets/css.json"
			},
			{
				"language": "less",
				"path": "./snippets/css.json"
			},
			{
				"language": "scss",
				"path": "./snippets/css.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js",
		"publish": "vsce package && vsce publish"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.4.1",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"typescript": "^4.4.3"
	}
}